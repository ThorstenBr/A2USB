0 REM MacFly's Mouse Traps

10    HOME
20    VTAB 2:HTAB 10:PRINT "MacFly's Mouse Traps"
30    VTAB 7
120   HTAB 5:PRINT "1 - MOUSE.MOVE  (BASIC Example)"
100   HTAB 5:PRINT "2 - MOUSE.DRAW  (BASIC Example)"
110   HTAB 5:PRINT "3 - MOUSE.HDRAW (HGR variant)"
130   HTAB 5:PRINT "4 - MOUSE.TEST  (ASM Example)"
130   HTAB 5:PRINT "5 - MOUSE.CLAMP (ASM Example)"
130   HTAB 5:PRINT "6 - MOUSE.DEBUG (DBG)"

200   PRINT "":PRINT "":PRINT ""
210   HTAB 5:PRINT "YOUR CHOICE: ";
220   GET A$
300   IF A$="1" THEN 1100
310   IF A$="2" THEN 1200
320   IF A$="3" THEN 1300
330   IF A$="4" THEN 1400
330   IF A$="5" THEN 1500
330   IF A$="6" THEN 1600
340   IF A$="0" THEN HOME:PRINT "BYE.":END
400   GOTO 220

1100 PRINT CHR$(4)"RUN MOUSE.MOVE"
1110 GOTO 1100
1200 PRINT CHR$(4)"RUN MOUSE.DRAW"
1210 GOTO 1200
1300 PRINT CHR$(4)"RUN MOUSE.HDRAW"
1310 GOTO 1300
1400 PRINT CHR$(4)"BRUN APPLEMOUSE.TEST"
1410 GOTO 1400
1500 PRINT CHR$(4)"BRUN MOUSE.CLAMP"
1510 GOTO 1500
1600 PRINT CHR$(4)"BRUN MOUSE.DEBUG"
1610 GOTO 1600
